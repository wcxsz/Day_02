<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.ShangChuanActivity">
    <Button
        android:id="@+id/bt_ok_shangchuan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="OK上传"
        />
    <Button
        android:id="@+id/bt_retrofit_shangchuan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="retrofit上传"
        />
    <Button
        android:id="@+id/bt_httpurl_shangchuan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="httpurl上传"
        />
    <ImageView
        android:id="@+id/iv_shangchuan"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>